{
  "name": "ld-didok",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:zazuko/ld-didok.git"
  },
  "dependencies": {
    "barnard59": "git+https://github.com/zazuko/barnard59.git#master",
    "json": "^9.0.6",
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {
  },
  "scripts": {
    "clean": "rm -rf target/*",
    "fetch-didok": "curl -s `node node_modules/json/lib/json.js url -f input/didok.csv-metadata.json` -o input/didok.csv",
    "fetch-rollmaterial": "curl -s `node node_modules/json/lib/json.js url -f input/rollmaterial.csv-metadata.json` -o input/rollmaterial.csv",
    "convert": "mkdir -p target && node index.js",
    "clean-didok": "cat target/didok.nt | sed '/_:/d' | sed '/<file/d' > target/didok-clean.nt",
    "clean-rollmaterial": "cat target/rollmaterial.nt | sed '/_:/d' | sed '/<file/d' > target/rollmaterial-clean.nt",
    "tdb-load": "tdbloader --loc=target/tdb_bernmobil target/didok-clean.nt target/rollmaterial-clean.nt",
    "tdb-query": "./scripts/tdb-query.sh",
    "tdb-dump": "./scripts/tdb-dump.sh",
    "validate-riot": "riot --validate target/everything.nt",
    "bernmobil-lindas-test": "env ENDPOINT=https://test.lindas-data.ch:8443/lindas ./scripts/lindas-put-everything.sh",
    "build": "npm-run-all clean fetch-didok fetch-rollmaterial convert clean-didok clean-rollmaterial tdb-load tdb-query tdb-dump validate-riot"
  }
}

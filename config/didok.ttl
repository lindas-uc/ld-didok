@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms:    <http://www.w3.org/ns/adms#>.
@prefix locn:    <http://www.w3.org/ns/locn#>.
@prefix schema: <http://schema.org/>.
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/>.
@prefix gont: <https://gont.ch/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix bafu: <http://example.org/> .


<#Pollutant>
  rml:logicalSource [
    rml:source "input/160128_BAV0116.csv";
    rml:referenceFormulation ql:CSV
  ]; 

  rr:subjectMap [
    rr:template "http://linked.transport.swiss/didok/{Dst-Nr.}";
    rr:class schema:CivicStructure ;
  ];

    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "Dst-Bezeichung offiziell";
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate schema:alternateName;
    rr:objectMap [
      rml:reference "Dst-Abk";
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate gont:id;
    rr:objectMap [
      rml:reference "Dst-Nr.";
      rr:datatype xsd:int 
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate gont:lv03_y;
    rr:objectMap [
      rml:reference "KOORDY" ;
      rr:datatype xsd:float 
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate gont:lv03_x;
    rr:objectMap [
      rml:reference "KOORDX" ;
      rr:datatype xsd:float 
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate gont:municipality;
    rr:objectMap [
      rr:template "http://classifications.data.admin.ch/municipality/{Gde-Nr}"
    ]
  ] .
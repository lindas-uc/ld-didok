@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms:    <http://www.w3.org/ns/adms#>.
@prefix locn:    <http://www.w3.org/ns/locn#>.
@prefix schema: <http://schema.org/>.
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/>.
@prefix gont: <https://gont.ch/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix bafu: <http://example.org/> .


<#Pollutant>
	rml:logicalSource [
    	rml:source "input/service.csv";
    	rml:referenceFormulation ql:CSV;
  	]; 

	rr:subjectMap [
    	rr:template "http://linked.transport.swiss/service/{ID}";
		rr:class schema:Service ;
	];

	rr:predicateObjectMap [
    	rr:predicate gont:didok;
    	rr:objectMap [
      		rr:template "http://linked.transport.swiss/didok/{didok}"
    	]
  	];

	rr:predicateObjectMap [
	    rr:predicate schema:name;
	    rr:objectMap [
			rml:reference "stationsbezeichnung";
			rr:datatype xsd:string
	    ]
	];

	rr:predicateObjectMap [
		rr:predicate schema:serviceType;
		rr:objectMap [
			rml:reference "service";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "periodevon";
			rr:datatype xsd:date
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "periodebis";
			rr:datatype xsd:date
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "von1";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "bis1";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "von2";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "bis2";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "von3";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:openingHours;
		rr:objectMap [
			rml:reference "bis3";
			rr:datatype xsd:dateTime
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Mo";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Di";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Mi";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Do";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Fr";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Sa";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "So";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Oeffnungsdauer";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Dauer Mo-Fr";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Dauer Sa";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Dauer So";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:provider;
		rr:objectMap [
			rml:reference "Unternehmung";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:areaServed;
		rr:objectMap [
			rml:reference "geopos";
		]
	];

	rr:predicateObjectMap [
		rr:predicate gont:id;
		rr:objectMap [
			rml:reference "ID";
			rr:datatype xsd:int
		]
	].
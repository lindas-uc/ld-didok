		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Perron";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Status";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate gont:id;
		rr:objectMap [
			rml:reference "FID";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate schema:geo;
		rr:objectMap [
			rml:reference "geopos";
		]
	].
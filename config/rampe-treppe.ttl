@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms:    <http://www.w3.org/ns/adms#>.
@prefix locn:    <http://www.w3.org/ns/locn#>.
@prefix schema: <http://schema.org/>.
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/>.
@prefix gont: <https://gont.ch/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix bafu: <http://example.org/> .


<#Pollutant>
	rml:logicalSource [
    	rml:source "input/rampe-treppe.csv";
    	rml:referenceFormulation ql:CSV;
  	]; 

	rr:subjectMap [
    	rr:template "http://linked.transport.swiss/rampe-treppe/{FID}";
		rr:class schema:CivicStructure ;
	];

	rr:predicateObjectMap [
    	rr:predicate gont:lv03_y;
    	rr:objectMap [
      		rml:reference "y" ;
      		rr:datatype xsd:float 
    	]
  	];

    rr:predicateObjectMap [
    	rr:predicate gont:lv03_x;
    	rr:objectMap [
      		rml:reference "x" ;
      		rr:datatype xsd:float 
    	]
  	];
  
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Linie";
			rr:datatype xsd:int
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Km";
			rr:datatype xsd:float
		]
	];

	rr:predicateObjectMap [
	    rr:predicate schema:name;
	    rr:objectMap [
			rml:reference "Name";
			rr:datatype xsd:string
	    ]
	];

	rr:predicateObjectMap [
	    rr:predicate rdfs:label;
	    rr:objectMap [
			rml:reference "BPS";
			rr:datatype xsd:string
	    ]
	];

	rr:predicateObjectMap [
	    rr:predicate rdfs:label;
	    rr:objectMap [
			rml:reference "BPS-Name";
			rr:datatype xsd:string
	    ]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Oben";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Unten";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "B-Jahr";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Typ";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Bauart";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Bem Bauart";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Nutzung";
			rr:datatype xsd:string
		]
	];


		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Eigentum";
			rr:datatype xsd:string
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Bem Eigentum";
			rr:datatype xsd:string
		]
